{
  "compilerOptions": {
    "target": "ES6",
    "module": "es6",
    "moduleResolution": "node",
    "types": ["node", "electron"],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "typeRoots": ["./typings", "./node_modules/"]
  },
  "include": ["src/**/*.ts", "typings/**/*.d.ts"]
}
